    C_ijkl = '5 10 5'
    fill_method = 'general_isotropic'
  []
  [Redbackcosserat]
    # 0 2.6549E3 2.6549E3
    type = RedbackMechMaterialHO
    poisson_ratio = '-9999'
    youngs_modulus = '-9999'
    block = '0'
    B_ijkl = '0 5.0E4 5.0E4'
    C_ijkl = '1.3333E3 4.0E3 2.0E3'
