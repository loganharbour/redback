[Preconditioning]
  inactive = 'debug_jacob default'
  [andy]
    type = SMP
    full = true
    petsc_options_iname = '-ksp_type -pc_type -snes_atol -snes_rtol -snes_max_it -ksp_max_it -sub_pc_type -sub_pc_factor_shift_type'
    petsc_options_value = 'gmres asm 1E-6 1E-10 30 500 lu NONZERO'
    line_search = basic
  []
  [debug_jacob]
    type = FDP
    full = true
  []
  [default]
    type = SMP
    line_search = 'basic'
    solve_type = NEWTON
  []
[]

[Executioner]
  type = Transient
  dt = 2
  solve_type = NEWTON
  num_steps = 10
  nl_abs_tol = 1e-8
  l_tol = 1e-10
  nl_rel_tol = 1e-04
[]

[Outputs]
  execute_on = 'timestep_end initial'
  exodus = true
  file_base = test_mech_dissip
  print_linear_residuals = false
